<html>
<head>
  <title>3VOT - <%= @profile.attributes.name %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" media="screen" href="https://netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
  <style>
html {
    background-color: #fff;
  }

  body {
      line-height: 1.5;
      height:100%;
      background: url(data:image/svg+xml;base64,);
      background-repeat: repeat-x;
      background-attachment:fixed;
      color: #4d4d4d;
      padding: 40px 0;
      font-family: 'Open Sans', sans-serif;
    }

    .navbar {
      min-height: 0;
      padding: 3px 0;
    }

    .form-control {
      border-radius: 3px!important;
      border: none;
      color: #4d4d4d;
      -webkit-box-shadow: inset 0 2px #e6e6e6;
      -moz-box-shadow: inset 0 2px #e6e6e6;
      box-shadow: inset 0 2px #e6e6e6;
    }

    .input-group-addon {
      position:relative; 
      background-color: transparent;
      border: none;
      color:#b3b3b3;
    }

    .btn {
      border-radius: 0;
      border: none;
      color: #4d4d4d;
      -webkit-transition: all .3s;
      -moz-transition: all .3s;
      -ms-transition: all .3s;
      -o-transition: all .3s;
      transition: all .3s;
    }

    .btn-default {
      -webkit-box-shadow: 0 2px 0 #e6e6e6;
      -moz-box-shadow: 0 2px 0 #e6e6e6;
      box-shadow: 0 2px 0 #e6e6e6;
    }

    .btn-default:hover, .btn-default:focus, .btn-default:active, .btn-default.active, .open .dropdown-toggle.btn-default {
      color: #4d4d4d; 
      background-color: #fff;
      -webkit-box-shadow: 0 2px 2px rgba(0,0,0,.2);
      -moz-box-shadow: 0 2px 2px rgba(0,0,0,.2);
      box-shadow: 0 2px 2px rgba(0,0,0,.2);
    }

    .btn-lg {
      font-size:14px;
    }

    .navbar-default {
      background-image: url(data:image/svg+xml;base64,);
      background-repeat: repeat-x;
      -webkit-background-size: 800px 42px;
      background-size: 800px 42px;
      border-bottom: 2px solid #cccccc;
    }

    a {
    }
    a:hover {
      text-decoration: none;
    }

    .appstore-search-affix {
      width: 60%;
      z-index: 1;
      padding: 0px 0px 3px 15px;
      box-shadow: 0 5px 2px rgba(245,245,245,.2);
      background: #f3f3f3; /* Old browsers */
background: -moz-linear-gradient(top, #f3f3f3 0%, #f5f5f5 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f3f3f3), color-stop(100%,#f5f5f5)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #f3f3f3 0%,#f5f5f5 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #f3f3f3 0%,#f5f5f5 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #f3f3f3 0%,#f5f5f5 100%); /* IE10+ */
background: linear-gradient(to bottom, #f3f3f3 0%,#f5f5f5 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f3f3f3', endColorstr='#f5f5f5',GradientType=0 ); /* IE6-9 */

    }

    .scroll-toid { 
      margin-top: -100px; 
      padding-bottom: 100px; 
      display: block; 
    }

    .appstore {
      padding: 10px 20px 30px;
    }

    .appstore-app-container {
      width: 210px;
      display: inline-block;
      margin:10px;
    }

    .appstore-app {
      color: #fff;
      padding: 20px;
      width: 100%;
      height: 210px;
      display: inline-block;
      box-shadow: 0 1px 2px rgba(0,0,0,.2);
      overflow: hidden;
      position: relative;
    }
    .appstore-app-icon {
      width:35%;
      margin-top: 25px;
    }
    .appstore-app-description {
      font-size:12px;
      margin-top: 100px;
      opacity: 0
    }
    .appstore-app-icon,
    .appstore-app-name,
    .appstore-app-description,
    .appstore-app-date{
      -webkit-transition: all .5s ease-in-out ;
      -moz-transition: all .5s ease-in-out ;
      -ms-transition: all .5s ease-in-out ;
      -o-transition: all .5s ease-in-out ;
      transition: all .5s ease-in-out ;
    }
    .appstore-app-icon {
      opacity: 1;
      transition-delay: .1s;
      -webkit-transition-delay: .1s; /* Safari */
    }
    .appstore-app:hover {
      color:#fff;
    }

    .appstore-app:hover .appstore-app-icon,
    .appstore-app:hover .appstore-app-date {
      margin-top:-150px;
      opacity: 0
    }

    .appstore-app:hover .appstore-app-description{
      margin-top: 0;
      opacity: 1
    }

    .public {
      background-color: #46c5e8;
    }
    .private {
      background-color: #ed705f;
    }

    ul {
      padding-left: 10px;
    }
    ul li {
      list-style: none
    }
    
    .logo {
      background: url(data:image/svg+xml;base64,);
      background-repeat: no-repeat;
     }
     
     .icon-marketplace {
       background: url(data:image/svg+xml;base64,);
       background-repeat: no-repeat;
       background-size:cover;
     }
    
    .icon-store {
      background: url(data:image/svg+xml;base64,);
      background-repeat: no-repeat;
      background-size:cover;
    }
    
    .icon-app {
      background: url(data:image/svg+xml;base64,);
      background-repeat: no-repeat;
      background-size:cover;
    }
   
  </style>
</head>
<body>
<div class="container">

    <div class="row">
      <div class="navbar navbar-default navbar-fixed-top " role="navigation">
        <div class="container">
          <div class="col-md-12 col-sm-12">
            <div class="col-md-3 col-sm-3">
             <a class="navbar-brand logo" style="top:-12px;width: 75px;" href="#"></a>
              <a href="#" class="navbar-link pull-left" style="position:relative; top:2px; font-size:12px; color: #fff">Marketplace <span class="icon-marketplace" style="margin:0 10px;height: 29px;width: 17px;display: inline-block;vertical-align: middle;"></span></a>
            </div>
            <div class="input-group col-md-9 col-sm-9">
              <input type="text" class="form-control " placeholder="Search for Organizations and Enterprises" value="">
              <span class="input-group-addon text-muted glyphicon glyphicon-search" style="display: inline-block;top: 4px;margin-left:-40px;"></span>
            </div>
          </div>
        </div>
      </div>
    </div>


      <div class="row">
        <div class="col-md-12 col-sm-12">
          
          <div class="col-md-3 col-sm-3 profile-sidebar-3vot" >
            <div class="profile-info-3vot" data-spy="affix" data-offset-top="0">
              <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-1">
                  <h2 class="" style=""><%= @profile.attributes.name %></h2>
                  <br>
                </div>
              </div>

              <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-1">
                  <div class="text-center pull-left" style="margin: 0 5%">
                    <div class="icon-store" style="height: 40px;width: 30px;margin: auto;"></div>
                    <span style="color:#ffb83b; font-size:12px">2 Stores</span>
                  </div>
                  <div class=" text-center pull-left" style="margin: 0 5%">
                   <div class="icon-app" style="height: 40px;width: 30px;margin: auto;"></div>
                    <span style="color:#1cb5ea; font-size:12px">2 Apps</span>
                  </div>
                </div>
              </div>

              <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-1" >
                  <p class="text-muted">
                    <br>                
                    Write a small introduction  for your profle &nbsp;  <span class="glyphicon glyphicon-pencil"></span>
                    <br><br>
                  </p>
                </div>
              </div>

              <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-offset-1" style="border-bottom:1px solid #e6e6e6"><span></span></div>
              </div>

              <div class="row">
                <div class="col-md-12 text-center col-sm-10">
                  <br>
                  <div class="icon-store" style="display: inline-block; width: 50px; height: 60px; vertical-align: middle;"></div>
                  <h4 style="color:#ffb83b;display: inline-block;">Stores</h4>
                  <br><br>
                </div>
              </div>

              <div class="row">
                <div class="col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-1">
                  <% for store in @stores: %>

                    <a href="#<%= store.attributes.name %>"class="btn btn-default btn-block"><%= store.attributes.name %></a>
                  <% end %>
                </div>
              </div>

            </div>
          </div>
          
          <div class="col-md-9 col-sm-9" >
              <div class="appstore-search" data-spy="affix" data-offset-top="0"><!-- Affix -->
              <div class="" style="margin-top:15px;" >
                <div class="row">
                  <div class="" >
                    <h4 style="margin-top:8px; color:#1cb5ea;" class="pull-left">
                      <span class="glyphicon glyphicon-search pull-left" style="margin: 0 10px;"></span>
                      Search for Apps &nbsp;
                    </h4>
                  </div>
                  <div class="input-group">
                    <input type="text" class="form-control " placeholder="Write an the name of an app or category" value="">
                    <span class="input-group-addon glyphicon glyphicon-search" style="position:relative; left:-40px; top: 1px;"></span>
                  </div>
                </div>
              </div> 
              </div>

            <section style="padding-top:70px;padding-bottom:200px">
          
              <div class="row">
                <div class="col-md-12 col-sm-12">
                  <% for store in @stores: %>

                  <div class="row">
                    <div class="col-md-12 col-sm-12">
                      <h5 style="font-weight:600;" class="appstore-name"><span class="scroll-toid" id="<%= store.attributes.name %>"> &nbsp;</span><span class="icon-store" style="height: 40px;width: 30px;display: inline-block;vertical-align: middle;margin: 0 10px;"></span><%= store.attributes.name %></h5>
                    </div>
                  </div>

                  <div class="appstore">
                     <div class="row">
                      <div class="col-md-12 col-sm-12">

                      <% for app in store.attributes?.apps: %>

                        <div class="appstore-app-container">
                          <a href="<%= app.name + "?store=" + store.attributes.name %>" class="appstore-app public text-center" >
                            <span class="appstore-app-icon icon-app" style="height: 65px;width: 55px;display: inline-block;"></span>
                            <h4 class="appstore-app-name"><%= app.name %></h4>
                            <p class="appstore-app-description"><%= app.description %></p>
                          </a>
                          <h6 class="text-muted text-center" style="margin:0px; color:#cccccc"><%= @profile.attributes.name %></h6>
                        </div>

                      <% end %>

                      </div>
                    </div>
                  </div>

                  <% end %>
                 
                </div>
              </div>

            </section>
          </div>

        </div>
      </div>
  </div><!-- Container -->

    <script src="https://code.jquery.com/jquery.js"></script>
  <script>

    $( document ).ready(function() {
 
      $('.appstore-search').on('affix.bs.affix', function (e) {
        $('.appstore-search').addClass('appstore-search-affix');
      })

      $('.appstore-search').on('affixed-top.bs.affix', function (e) {
        $('.appstore-search').removeClass('appstore-search-affix');
      })
 
    });

  </script>
  <script src="https://netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>
</body>
</html>
